#
# "logFC"	"logCPM"	"LR"	"PValue"	"FDR"
# "ENSG00000169045"	1.28240399003135	14.1576397877527	300.094946042136	3.14112946315903e-67	1.78918734221538e-63


awk '
 BEGIN{FS = "\t";
        OFS = "\t";
      }
 {
   if($2 < 0)
     c = 1.0;
   else if($2 == 0)
     c = 0;
   else
     c = -1.0;
   printf("%s\t%f\n", $1, c* log($5) / log(10) );
 }
 ' "$1" 
