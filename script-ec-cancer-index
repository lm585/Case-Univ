df <- as.data.frame(pbmc.bcell.pos$ADT@counts)
rownames(df) <- paste("adt--", rownames(df), sep = "")
pbmc.bcell.pos[["newADT"]] <- CreateAssayObject(df)

pbmc.bcell.pos$RNA@counts <- rbind(pbmc.bcell.pos$RNA@counts, pbmc.bcell.pos$newADT@counts)
pbmc.bcell.pos$RNA@data   <- rbind(pbmc.bcell.pos$RNA@data, pbmc.bcell.pos$newADT@data) 
dim(pbmc.bcell.pos)
### 23373 10132
splitLS <- SplitObject(object = pbmc.bcell.pos, split.by = "orig.ident")
splitLS <- lapply(X = splitLS, FUN = function(x) {
    x <- NormalizeData(x, verbose = T)
    x <- FindVariableFeatures(x, selection.method = "vst", nfeatures = 2000, verbose = T)
})
intFeats <- SelectIntegrationFeatures(splitLS)
intFeats <- setdiff(intFeats, row.names(pbmc.bcell.pos@assays$newADT))
splitAnchors <- FindIntegrationAnchors(object.list = splitLS,
                                         dims = 1:30, k.filter = 200, anchor.features = c(intFeats, row.names(pbmc.bcell.pos@assays$newADT)))
###Retained 3371 anchors
pbmc.bcell.pos.integrated <- IntegrateData(anchorset = splitAnchors, dims = 1:30)
pbmc.bcell.pos.integrated <- ScaleData(pbmc.bcell.pos.integrated, verbose = T)
pbmc.bcell.pos.integrated <- RunPCA(pbmc.bcell.pos.integrated, npcs = 30, verbose = T)
pbmc.bcell.pos.integrated <- RunUMAP(pbmc.bcell.pos.integrated, reduction = "pca", dims = 1:30)
###pbmc.bcell.pos.integrated.fine <- pbmc.bcell.pos.integrated[, (grepl(pattern = "B cells", pbmc.bcell.pos.integrated$Ethnicity.num)) | pbmc.bcell.pos.integrated$Ethnicity.num == "Plasmablasts"]  #### 2101 10734; 10734 / 11024  = 97%
###DimPlot(pbmc.bcell.pos.integrated.fine, reduction = "umap", group.by = "Ethnicity.num", cols = c("red", "yellow", "black", "blue", "green"))
## cols = c("purple", "yellow", "orange", "green", "black", "blue", "red", "yellow", "red", "blue")
