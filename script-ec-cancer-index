$ head temp.txt
"primerid"	"logFC"
"GRCh38.99-------------ACTB"	0.523520958290465

genes <- read.delim("temp.txt", header = T)
top50 = genes
splitAnchors <- FindIntegrationAnchors(object.list = splitLS,
                                         dims = 1:30, k.filter = 35, anchor.features = unique(c(intFeats, row.names(bCombBcell@assays$ADT), genes$primerid)))
bCombBcell.integrated <- IntegrateData(anchorset = splitAnchors, dims = 1:30)
bCombBcell.integrated <- ScaleData(bCombBcell.integrated, verbose = T)
bCombBcell.integrated <- RunPCA(bCombBcell.integrated, npcs = 30, verbose = T)
bCombBcell.integrated <- RunUMAP(bCombBcell.integrated, reduction = "pca", dims = 1:30)
cl <- read.table("bal.bcell.covidPos.clust.startBelg.xls", header = T, sep = "\t") ### cells
for ( i in rownames(cl)) {bCombBcell.integrated$HTO_classification[i] = cl[i,1] }
dim(bCombBcell.integrated)
pb.integrated <- bCombBcell.integrated[, bCombBcell.integrated$HTO_classification == 1 | bCombBcell.integrated$HTO_classification == 3 | bCombBcell.integrated$HTO_classification == 5 | bCombBcell.integrated$HTO_classification == 8]
dim(pb.integrated)
mat <- as.matrix(pb.integrated$integrated@scale.data[genes$primerid,])
rownames(mat) <- gsub(pattern = ".+---", replacement = "g-", rownames(mat))
ha <- HeatmapAnnotation(sev = pb.integrated@meta.data$Severity.Score.worse )
Heatmap(matrix = mat,
        column_split = pb.integrated@meta.data$Severity.Score.worse,
        top_annotation = ha,
        show_column_names = FALSE,
