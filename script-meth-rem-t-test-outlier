### ls | grep "^ENSG" | while read f 
### do 
###  wc -l $f; 
### done | awk '
###  {
###   if($1 > 0)
###     print $2;
###  }
###  ' > temp-ENSG-files
### 
### cat temp-ENSG-files genes-constant-meth-t-test-error.genes.txt | sort | uniq -u > temp-441pm
### mv temp-441pm temp-ENSG-files

ls -lh temp-ENSG-files 

cat temp-ENSG-files |  while read file
do
 cut -f 2 $file > temp-s1
 cut -f 6 $file >> temp-s1
 cut -f 3 $file > temp-s2
 cut -f 7 $file >> temp-s2
 
 paste temp-s1 temp-s2 | awk '{print $1 - $2}' | uniq  > temp-s3
 t=`wc -l temp-s3 | awk '{print $1}'`
 if [ $t -eq 1 ]
 then
   t2=`cat temp-s3`
   if [ "$t2" != "0" ]
   then
    echo "$file"
    cat "$file" | cut -f 2,3,6,7,9,10,11 
   fi
 fi
done
